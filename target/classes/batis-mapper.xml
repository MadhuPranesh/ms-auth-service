<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cognizant.microservices.authservice">
    <select id="selectSample" resultType="com.cognizant.microservices.authservice.model.SampleUser" parameterType="java.util.Map">
        SELECT * FROM sample where id = #{id,jdbcType=INTEGER}
    </select>
    <insert id="insertSample" parameterType="com.cognizant.microservices.authservice.model.SampleUser">
        INSERT INTO SAMPLE(id,name,college) values(#{id,jdbcType=INTEGER},#{name,jdbcType=VARCHAR},#{college,jdbcType=VARCHAR})
    </insert>
</mapper>